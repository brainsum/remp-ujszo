{
	"name": "remp/crm-skeleton",
	"description": "REMP - CRM Skeleton",
	"type": "project",
	"homepage": "https://remp2020.com",
	"license": "MIT",
	"config": {
		"sort-packages": true
	},
	"minimum-stability": "stable",
	"prefer-stable": true,
	"repositories": [
		{
			"type": "path",
			"url": "extensions/*",
			"options": {
				"symlink": true
			}
		}
	],
	"require": {
		"remp/crm-admin-module": "~0.15",
		"remp/crm-api-module": "~0.15",
		"remp/crm-application-module": "~0.15",
		"remp/crm-dashboard-module": "~0.15",
		"remp/crm-gifts-module": "~0.15",
		"remp/crm-invoices-module": "~0.15",
		"remp/crm-issues-module": "~0.15",
		"remp/crm-onboarding-module": "~0.15",
		"remp/crm-payments-module": "~0.15",
		"remp/crm-print-module": "~0.15",
		"remp/crm-products-module": "~0.15",
		"remp/crm-salesfunnel-module": "~0.15",
		"remp/crm-scenarios-module": "~0.15",
		"remp/crm-segment-module": "~0.15",
		"remp/crm-stripe-module": "^0.15.0",
		"remp/crm-subscriptions-module": "~0.15",
		"remp/crm-upgrades-module": "~0.15",
		"remp/crm-users-module": "~0.15",
		"remp/crm-remp-mailer-module": "~0.15"
	},
	"scripts": {
		"post-autoload-dump": [
			"Crm\\ApplicationModule\\ComposerScripts::postAutoloadDump"
		]
	}
}