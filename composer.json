{
	"name": "remp/crm-skeleton",
	"description": "REMP - CRM Skeleton",
	"type": "project",
	"homepage": "https://remp2020.com",
	"license": "MIT",
	"config": {
		"sort-packages": true
	},
	"minimum-stability": "stable",
	"prefer-stable": true,
	"repositories": [
		{
			"type": "path",
			"url": "extensions/*",
			"options": {
				"symlink": true
			}
		},
		{
			"type": "vcs",
			"url": "https://github.com/brainsum/crm-payments-module"
		},
		{
			"type": "vcs",
			"url": "https://github.com/brainsum/crm-salesfunnel-module"
		},
		{
			"type": "vcs",
			"url": "https://github.com/brainsum/crm-subscriptions-module"
		},
		{
			"type": "vcs",
			"url": "https://github.com/brainsum/crm-invoices-module"
		},
		{
			"type": "vcs",
			"url": "https://github.com/brainsum/crm-users-module"
		}
	],
	"require": {
		"paypal/paypal-checkout-sdk": "1.0.1",
		"remp/crm-admin-module": "~0.9",
		"remp/crm-api-module": "~0.9",
		"remp/crm-application-module": "~0.12",
		"remp/crm-dashboard-module": "~0.12",
		"remp/crm-invoices-module": "dev-remp as 0.12.0",
		"remp/crm-issues-module": "~0.12",
		"remp/crm-onboarding-module": "~0.12",
		"remp/crm-payments-module": "dev-remp as 0.12.0",
		"remp/crm-salesfunnel-module": "dev-remp as 0.12.0",
		"remp/crm-scenarios-module": "~0.12",
		"remp/crm-segment-module": "~0.12",
		"remp/crm-subscriptions-module": "dev-remp as 0.12.0",
		"remp/crm-users-module": "dev-remp as 0.12.0",
		"ujszo/ujszo_module": "@dev"
	}
}
